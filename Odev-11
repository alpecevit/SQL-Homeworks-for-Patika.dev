-- 1.Soru Cevap --
SELECT first_name
FROM customer
UNION
SELECT first_name
FROM actor;

-- 2.Soru Cevap --
SELECT first_name
FROM customer
INTERSECT
SELECT first_name
FROM actor;

-- 3.Soru Cevap --
SELECT first_name
FROM actor
EXCEPT
SELECT first_name
FROM customer;

-- 4.Soru Cevap --
SELECT first_name
FROM customer
UNION ALL
SELECT first_name
FROM actor;

-- 4.Soru Cevap --
SELECT first_name
FROM customer
INTERSECT ALL
SELECT first_name
FROM actor;

-- 4.Soru Cevap --
SELECT first_name
FROM customer
EXCEPT ALL
SELECT first_name
FROM actor;
